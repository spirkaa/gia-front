{
  "name": "gia-front",
  "description": "React frontend for Django REST Framework",
  "version": "3.0.0",
  "license": "MIT",
  "author": "piv@devmem.ru",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node compress.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "prettier": {
    "printWidth": 88,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "brotli": "^1.3.2",
    "history": "^5.2.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "normalizr": "^3.6.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bootstrap": "^0.33.1",
    "react-bootstrap-table-next": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-redux-toastr": "^7.6.6",
    "react-router": "^6.2.1",
    "react-router-bootstrap": "^0.26.0",
    "react-router-dom": "^6.2.1",
    "react-ultimate-pagination": "^1.2.0",
    "redux": "^4.1.2",
    "redux-auth-wrapper": "^3.0.0",
    "redux-first-history": "^5.0.8",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-scripts": "5.0.0",
    "redux-logger": "^3.0.6"
  }
}
